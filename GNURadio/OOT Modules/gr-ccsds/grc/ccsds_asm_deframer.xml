<block>
  <name>CCSDS ASM Deframer</name>
  <key>ccsds_asm_deframer</key>
  <category>CCSDS</category>
  <import>import ccsds</import>
  <make>ccsds.asm_deframer($distance,$interleave,$randomise,$block_size)</make>
  <callback>set_distance($distance,$distance)</callback>
  <callback>set_interleave($interleave,$interleave)</callback>
  <callback>set_radnomise($randomise,$randomise)</callback>
  <callback>set_block_size($block_size,$block_size)</callback>
  <param>
    <name>Distance</name>
    <key>distance</key>
    <value>0</value>
    <type>int</type>
  </param>
  <param>
    <name>Interleave</name>
    <key>interleave</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Randomise</name>
    <key>randomise</key>
    <value>False</value>
		<type>int</type>
		<option>
			<name>Yes</name>
			<key>1</key>
		</option>
		<option>
			<name>No</name>
			<key>0</key>
		</option>
  </param>
  <param>
    <name>Block Size</name>
    <key>block_size</key>
    <value>255</value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
